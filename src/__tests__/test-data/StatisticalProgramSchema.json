{
  "$ref": "#/definitions/StatisticalProgram",
  "definitions": {
    "MultilingualText": {
      "type": "object",
      "properties": {
        "languageCode": {
          "type": "string",
          "description": "The language code. Use only ISO 639-1 codes.",
          "displayName": "Language code",
          "enum": [
            "nb",
            "nn",
            "en"
          ]
        },
        "languageText": {
          "type": "string",
          "description": "The text (e.g. label, title, description)",
          "displayName": "Language text"
        }
      },
      "required": [
        "languageCode",
        "languageText"
      ],
      "additionalProperties": false
    },
    "AdministrativeDetails": {
      "type": "object",
      "properties": {
        "administrativeDetailType": {
          "type": "string",
          "description": "The type (key) of the AdministrativeDetails-attribute.",
          "displayName": "Administrative detail type",
          "enum": [
            "DEFAULTLANGUAGE",
            "ANNOTATION",
            "ALIAS",
            "URL",
            "DOCUMENTATION",
            "LOCALID",
            "ORIGIN"
          ]
        },
        "values": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "One or more values (a list) for this administrativeDetailType.",
          "displayName": "Values"
        }
      },
      "required": [
        "administrativeDetailType",
        "values"
      ],
      "additionalProperties": false
    },
    "StatisticalProgram": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "The global unique identifier (GUID) of the information object assigned by the owner agency.",
          "displayName": "Id"
        },
        "name": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/MultilingualText"
          },
          "description": "A term which designates a concept, in this case an information object. The identifying name will be the preferred designation. There will be many terms to designate the same information object, such as synonyms and terms in other languages.",
          "displayName": "Name"
        },
        "description": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/MultilingualText"
          },
          "description": "The description of the information object",
          "displayName": "Description"
        },
        "administrativeStatus": {
          "type": "string",
          "description": "Indicator for access to an item.",
          "displayName": "Administrative status",
          "enum": [
            "DRAFT",
            "INTERNAL",
            "OPEN",
            "DEPRECATED"
          ]
        },
        "createdDate": {
          "type": "string",
          "format": "date-time",
          "description": "The date on which the information object was created",
          "displayName": "Created date"
        },
        "createdBy": {
          "type": "string",
          "description": "Information object created by.",
          "displayName": "Created by"
        },
        "version": {
          "type": "string",
          "description": "Formal versioning of the information object. The version designator of the information object assigned by the owner agency. \"major.minor.patch\", e.g. \"1.12.5\".",
          "displayName": "Version"
        },
        "versionValidFrom": {
          "type": "string",
          "format": "date-time",
          "description": "The date on which the current version of the infomation object is effective or valid.",
          "displayName": "Version valid from"
        },
        "versionRationale": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/MultilingualText"
          },
          "description": "The reason for making this version of the information object.",
          "displayName": "Version rationale"
        },
        "lastUpdatedDate": {
          "type": "string",
          "format": "date-time",
          "description": "The date on which the object was last updated.",
          "displayName": "Last updated date"
        },
        "lastUpdatedBy": {
          "type": "string",
          "description": "Last updated by.",
          "displayName": "Last updated by"
        },
        "validFrom": {
          "type": "string",
          "format": "date-time",
          "description": "The date on which the information object is effective or valid. Set this date only once (constant). The same date for all versions of this information object.",
          "displayName": "Valid from"
        },
        "validUntil": {
          "type": "string",
          "format": "date-time",
          "description": "The date on which the information object is no longer effective or valid.",
          "displayName": "Valid until"
        },
        "administrativeDetails": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AdministrativeDetails"
          },
          "description": "Administrative details (e.g. default language, documentation, localID, ...).",
          "displayName": "Administrative details"
        },
        "agentInRoles": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "displayName": "Agent in roles",
          "description": "Agent(s) acting in the Role(s) for this information object."
        },
        "parentStatisticalPrograms": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "displayName": "Parent statistical programs"
        },
        "statisticalProgramDesign": {
          "type": "string",
          "displayName": "Statistical program design"
        },
        "statisticalProgramCycles": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "displayName": "Statistical program cycles"
        },
        "dateInitiated": {
          "type": "string",
          "format": "date-time",
          "description": "First date of validity",
          "displayName": "Date initiated"
        },
        "dateEnded": {
          "type": "string",
          "format": "date-time",
          "description": "Last date of validity",
          "displayName": "Date ended"
        },
        "statisticalProgramStatus": {
          "type": "string",
          "description": "The current condition of the program",
          "displayName": "Statistical program status",
          "enum": [
            "NEW",
            "UNDER_DEVELOPMENT",
            "CURRENT",
            "COMPLETED",
            "CANCELLED",
            "TRANSFERRED"
          ]
        },
        "history": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/MultilingualText"
          },
          "description": "A description of the precursors of the program within the organisation",
          "displayName": "History"
        },
        "subjectMatterDomains": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/MultilingualText"
          },
          "description": "A description of the subject matters",
          "displayName": "Subject matter domains"
        },
        "sourceOfFunding": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/MultilingualText"
          },
          "description": "A description of the source of funding",
          "displayName": "Source of funding"
        },
        "budget": {
          "type": "number",
          "description": "Budget",
          "displayName": "Budget"
        },
        "legislativeReferences": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/MultilingualText"
          },
          "description": "Any legislative materials, eg parliamentary tabling documents",
          "displayName": "Legislative references"
        },
        "legalFrameworks": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/MultilingualText"
          },
          "description": "Description of the legal framework",
          "displayName": "Legal frameworks"
        },
        "_link_property_agentInRoles": {
          "type": "object",
          "properties": {
            "AgentInRole": {
              "type": "null"
            }
          }
        },
        "_link_property_parentStatisticalPrograms": {
          "type": "object",
          "properties": {
            "StatisticalProgram": {
              "type": "null"
            }
          }
        },
        "_link_property_statisticalProgramDesign": {
          "type": "object",
          "properties": {
            "StatisticalProgramDesign": {
              "type": "null"
            }
          }
        },
        "_link_property_statisticalProgramCycles": {
          "type": "object",
          "properties": {
            "StatisticalProgramCycle": {
              "type": "null"
            }
          }
        }
      },
      "required": [
        "id",
        "name",
        "createdDate",
        "createdBy",
        "versionValidFrom",
        "validFrom",
        "statisticalProgramStatus",
        "history",
        "subjectMatterDomains"
      ],
      "description": "A set of activities, which may be repeated, to investigate characteristics of a given Population. It describes the purpose and context of a set of Business Process within the context of the relevant Statistical Program Cycles.",
      "displayName": "Statistical program",
      "additionalProperties": false
    }
  },
  "$schema": "http://json-schema.org/draft-04/schema#"
}