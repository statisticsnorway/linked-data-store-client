{
  "name": "linked-data-store-client",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/statisticsnorway/linked-data-store-client"
  },
  "dependencies": {
    "chart.js": "^2.7.3",
    "date-fns": "^2.0.0-alpha.27",
    "react": "^16.7.0",
    "react-chartjs-2": "^2.7.4",
    "react-datepicker": "^2.1.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.0",
    "react-table": "^6.9.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.86.0",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose=false",
    "coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "jest-dom": "^3.1.2",
    "react-testing-library": "^7.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**",
      "!src/index.js",
      "!src/**/index.js",
      "!src/enum/**",
      "!src/utilities/fetch/Fetch.js",
      "!src/__tests__/test-data/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 25,
        "functions": 25,
        "lines": 25,
        "statements": 25
      }
    }
  }
}
